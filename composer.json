{
  "name": "michaelj2324/sugarcrm-restv10-sdk",
  "description": "An SDk for interacting with Sugar's REST v10 API.",
  "license": "MIT",
  "authors": [
    {
      "name": "Mike Russell",
      "email": "mr@mikejrussell.com",
      "role": "Developer"
    }
  ],
  "keywords": [
    "sugarcrm",
    "sugar",
    "crm",
    "rest",
    "sdk",
    "v10",
    "api"
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/MichaelJ2324/sugarcrm-restv10-sdk.git"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0|~5.0"
  },
  "autoload": {
    "psr-4": {
      "SugarAPI\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SugarAPI\\SDK\\Tests\\": "tests/"
    }
  }
}
